{
  "expo": {
    "name": "T Ride",
    "slug": "trav-ride",
    "owner": "travoney_1",
    "version": "3.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-rider.png",
    "scheme": "travony-rider",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "extra": {
      "appVariant": "rider",
      "eas": {
        "projectId": "366f0f57-2f25-4053-a69b-1905365f8013"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.travony.rider"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#00B14F",
        "foregroundImage": "./assets/images/android-icon-rider-foreground.png",
        "backgroundImage": "./assets/images/android-icon-rider-background.png",
        "monochromeImage": "./assets/images/android-icon-rider-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.travony.rider",
      "versionCode": 38,
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_API_KEY}"
        }
      }
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon-rider.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-rider.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00B14F",
          "dark": {
            "backgroundColor": "#1A1A1A"
          }
        }
      ],
      "expo-web-browser",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.travony.rider",
          "enableGooglePay": true
        }
      ]
    ],
    "experiments": {
      "reactCompiler": false
    }
  }
}
